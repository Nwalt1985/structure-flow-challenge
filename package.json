{
  "name": "pricing-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "prettier": "prettier --list-different './**/*.{js,ts}' --config '../.prettierrc.json' --ignore-path '../.prettierignore'",
    "prettier-write": "prettier --list-different './**/*.{js,ts}' --config '../.prettierrc.json' --ignore-path '../.prettierignore' --write",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "start": "clear && serverless offline start",
    "dynamodb-admin": "clear && dynamodb-admin"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.38.0",
    "@types/aws-lambda": "^8.10.142",
    "@types/node": "^14.18.63",
    "@types/parse-multipart": "^1.0.0",
    "@typescript-eslint/parser": "^5.30.0",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^8.8.0",
    "serverless-plugin-typescript": "^2.1.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1167.0",
    "axios": "^0.27.2",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.1",
    "dynamodb": "^1.3.0",
    "http-status-codes": "^2.2.0",
    "parse-multipart": "^1.0.4",
    "serverless-api-gateway-caching": "^1.8.1",
    "serverless-dynamodb-autoscaling": "^0.6.2"
  }
}
