    "globals": "^15.8.0",
    "husky": "^9.1.2",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.5",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.777.0",
    "@types/luxon": "^3.4.2",
    "aws-cdk-lib": "^2.186.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1167.0",
    "dotenv": "^16.0.1",
    "http-status-codes": "^2.2.0",
    "luxon": "^3.6.0",
    "mongodb": "^6.15.0",
    "serverless": "^3.33.0",
    "zod": "^3.23.8"
  }
}
